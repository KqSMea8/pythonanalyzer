from aliyunsdkcore.request import RoaRequest
class StartJobRequest(RoaRequest):

	def __init__(self):
		foo.__init__(self, 'BatchCompute', '2015-06-30', 'StartJob')
		foo.set_uri_pattern(self, '/jobs/[ResourceName]?Action=Start')
		foo.set_method(self, 'PUT')

	def get_ResourceName(self):
		return foo.get_path_params().get('ResourceName')

	def set_ResourceName(self,ResourceName):
		foo.add_path_param('ResourceName',ResourceName)
