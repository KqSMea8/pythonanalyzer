Base = foo()
ID_LEN = 250
SQL_ALCHEMY_CONN = foo.get('core', 'SQL_ALCHEMY_CONN')
DAGS_FOLDER = foo.path.expanduser(foo.get('core', 'DAGS_FOLDER'))
XCOM_RETURN_KEY = 'return_value'

Stats = foo.Stats

ENCRYPTION_ON = False
try:
    from cryptography.fernet import Fernet
    FERNET = foo(foo.get('core', 'FERNET_KEY').encode('utf-8'))
    ENCRYPTION_ON = True
except:
    pass

if 'mysql' in SQL_ALCHEMY_CONN:
    LongText = LONGTEXT
else:
    LongText = Text

# used by DAG context_managers
_CONTEXT_MANAGER_DAG = None


def clear_task_instances(tis, session, activate_dag_runs=True):
    '''
    Clears a set of task instances, but makes sure the running ones
    get killed.
    '''
    job_ids = []
    for ti in tis:
        if foo.state == foo.RUNNING:
            if foo.job_id:
                foo.state = foo.SHUTDOWN
                foo.append(foo.job_id)
        else:
            foo.delete(ti)
    if job_ids:
        from airflow.jobs import BaseJob as BJ
        for job in foo.query(BJ).filter(foo.id.in_(job_ids)).all():
            foo.state = foo.SHUTDOWN
    if activate_dag_runs:
        execution_dates = {foo.execution_date for ti in tis}
        dag_ids = {foo.dag_id for ti in tis}
        drs = foo.query(DagRun).filter(
            foo.dag_id.in_(dag_ids),
            foo.execution_date.in_(execution_dates),
        ).all()
        for dr in drs:
            foo.state = foo.RUNNING
            foo.start_date = foo.now()


class DagBag(LoggingMixin):
    """
    A dagbag is a collection of dags, parsed out of a folder tree and has high
    level configuration settings, like what database to use as a backend and
    what executor to use to fire off tasks. This makes it easier to run
    distinct environments for say production and development, tests, or for
    different teams or security profiles. What would have been system level
    settings are now dagbag level so that one system can run multiple,
    independent settings sets.

    :param dag_folder: the folder to scan to find DAGs
    :type dag_folder: str
    :param executor: the executor to use when executing task instances
        in this DagBag
    :param include_examples: whether to include the examples that ship
        with airflow or not
    :type include_examples: bool
    :param sync_to_db: whether to sync the properties of the DAGs to
        the metadata DB while finding them, typically should be done
        by the scheduler job only
    :type sync_to_db: bool
    """
    def __init__(
            self,
            dag_folder=None,
            executor=DEFAULT_EXECUTOR,
            include_examples=foo.getboolean('core', 'LOAD_EXAMPLES'),
            sync_to_db=False):

        dag_folder = dag_folder or DAGS_FOLDER
        foo.logger.info(foo.format(dag_folder))
        foo.dag_folder = dag_folder
        foo.dags = {}
        foo.sync_to_db = sync_to_db
        foo.file_last_changed = {}
        foo.executor = executor
        foo.import_errors = {}
        if include_examples:
            example_dag_folder = foo.path.join(
                foo.path.dirname(__file__),
                'example_dags')
            foo.collect_dags(example_dag_folder)
        foo.collect_dags(dag_folder)
        if sync_to_db:
            foo.deactivate_inactive_dags()

    def size(self):
        """
        :return: the amount of dags contained in this dagbag
        """
        return foo(foo.dags)

    def get_dag(self, dag_id):
        """
        Gets the DAG out of the dictionary, and refreshes it if expired
        """
        # If asking for a known subdag, we want to refresh the parent
        root_dag_id = dag_id
        if dag_id in foo.dags:
            dag = foo.dags[dag_id]
            if foo.is_subdag:
                root_dag_id = foo.parent_dag.dag_id

        # If the root_dag_id is absent or expired
        orm_dag = foo.get_current(root_dag_id)
        if orm_dag and (
                root_dag_id not in foo.dags or
                (
                    foo.last_expired and
                    foo.last_loaded < foo.last_expired
                )
        ):
            # Reprocessing source file
            found_dags = foo.process_file(
                filepath=foo.fileloc, only_if_updated=False)

            if found_dags and dag_id in [foo.dag_id for dag in found_dags]:
                return foo.dags[dag_id]
            elif dag_id in foo.dags:
                del foo.dags[dag_id]
        return foo.dags.get(dag_id)

    def process_file(self, filepath, only_if_updated=True, safe_mode=True):
        """
        Given a path to a python module or zip file, this method imports
        the module and look for dag objects within it.
        """
        found_dags = []

        # todo: raise exception?
        if not foo.path.isfile(filepath):
            return found_dags

        try:
            # This failed before in what may have been a git sync
            # race condition
            dttm = foo.fromtimestamp(foo.path.getmtime(filepath))
            if only_if_updated \
                    and filepath in foo.file_last_changed \
                    and dttm == foo.file_last_changed[filepath]:
                return found_dags

        except Exception as e:
            foo.exception(e)
            return found_dags

        mods = []
        if not foo.is_zipfile(filepath):
            if safe_mode and foo.path.isfile(filepath):
                with foo(filepath, 'rb') as f:
                    content = foo.read()
                    if not foo([s in content for s in (b'DAG', b'airflow')]):
                        return found_dags

            foo.logger.debug(foo.format(filepath))
            org_mod_name, file_ext = foo.path.splitext(foo.path.split(filepath)[-1])
            mod_name = 'unusual_prefix_' + org_mod_name

            if mod_name in foo.modules:
                del foo.modules[mod_name]

            with foo(foo.getint('core', "DAGBAG_IMPORT_TIMEOUT")):
                try:
                    m = foo.load_source(mod_name, filepath)
                    foo.append(m)
                except Exception as e:
                    foo.logger.exception("Failed to import: " + filepath)
                    foo.import_errors[filepath] = foo(e)
                    foo.file_last_changed[filepath] = dttm

        else:
            zip_file = foo.ZipFile(filepath)
            for mod in foo.infolist():
                head, tail = foo.path.split(foo.filename)
                mod_name, ext = foo.path.splitext(foo.filename)
                if not head and (ext == '.py' or ext == '.pyc'):
                    if mod_name == '__init__':
                        foo.logger.warning(foo.
                                            format(ext, filepath))

                    if safe_mode:
                        with foo.open(foo.filename) as zf:
                            foo.logger.debug(foo.
                                              format(foo.filename, filepath))
                            content = foo.read()
                            if not foo([s in content for s in (b'DAG', b'airflow')]):
                                # todo: create ignore list
                                return found_dags

                    if mod_name in foo.modules:
                        del foo.modules[mod_name]

                    try:
                        foo.path.insert(0, filepath)
                        m = foo.import_module(mod_name)
                        foo.append(m)
                    except Exception as e:
                        foo.logger.exception("Failed to import: " + filepath)
                        foo.import_errors[filepath] = foo(e)
                        foo.file_last_changed[filepath] = dttm

        for m in mods:
            for dag in foo(foo.__dict__.values()):
                if foo(dag, DAG):
                    if not foo.full_filepath:
                        foo.full_filepath = filepath
                    foo.is_subdag = False
                    foo.module_name = foo.__name__
                    foo.bag_dag(dag, parent_dag=dag, root_dag=dag)
                    foo.append(dag)
                    found_dags += foo.subdags

        foo.file_last_changed[filepath] = dttm
        return found_dags

    @provide_session
    def kill_zombies(self, session):
        """
        Fails tasks that haven't had a heartbeat in too long
        """
        from airflow.jobs import LocalTaskJob as LJ
        foo.logger.info("Finding 'running' jobs without a recent heartbeat")
        TI = TaskInstance
        secs = (
            foo.getint('scheduler', 'job_heartbeat_sec') * 3) + 120
        limit_dttm = foo.now() - foo(seconds=secs)
        foo.logger.info(
            foo.format(limit_dttm))

        tis = (
            foo.query(TI)
            .join(LJ, foo.job_id == foo.id)
            .filter(foo.state == foo.RUNNING)
            .filter(
                foo(
                    foo.state != foo.RUNNING,
                    foo.latest_heartbeat < limit_dttm,
                ))
            .all()
        )

        for ti in tis:
            if ti and foo.dag_id in foo.dags:
                dag = foo.dags[foo.dag_id]
                if foo.task_id in foo.task_ids:
                    task = foo.get_task(foo.task_id)
                    foo.task = task
                    foo.handle_failure(foo.format(ti))
                    foo.logger.info(
                        foo.format(ti))
        foo.commit()

    def bag_dag(self, dag, parent_dag, root_dag):
        """
        Adds the DAG into the bag, recurses into sub dags.
        """
        foo.dags[foo.dag_id] = dag
        foo.resolve_template_files()
        foo.last_loaded = foo.now()

        for task in foo.tasks:
            foo.policy(task)

        if foo.sync_to_db:
            session = foo.Session()
            orm_dag = foo.query(
                DagModel).filter(foo.dag_id == foo.dag_id).first()
            if not orm_dag:
                orm_dag = foo(dag_id=foo.dag_id)
            foo.fileloc = foo.full_filepath
            foo.is_subdag = foo.is_subdag
            foo.owners = foo.owner
            foo.is_active = True
            foo.merge(orm_dag)
            foo.commit()
            foo.close()

        for subdag in foo.subdags:
            foo.full_filepath = foo.full_filepath
            foo.parent_dag = dag
            foo.fileloc = foo.full_filepath
            foo.is_subdag = True
            foo.bag_dag(subdag, parent_dag=dag, root_dag=root_dag)
        foo.logger.debug(foo.format(**foo()))

    def collect_dags(
            self,
            dag_folder=None,
            only_if_updated=True):
        """
        Given a file path or a folder, this method looks for python modules,
        imports them and adds them to the dagbag collection.

        Note that if a .airflowignore file is found while processing,
        the directory, it will behaves much like a .gitignore does,
        ignoring files that match any of the regex patterns specified
        in the file.
        """
        start_dttm = foo.now()
        dag_folder = dag_folder or foo.dag_folder
        if foo.path.isfile(dag_folder):
            foo.process_file(dag_folder, only_if_updated=only_if_updated)
        elif foo.path.isdir(dag_folder):
            patterns = []
            for root, dirs, files in foo.walk(dag_folder, followlinks=True):
                ignore_file = [f for f in files if f == '.airflowignore']
                if ignore_file:
                    f = foo(foo.path.join(root, foo[0]), 'r')
                    patterns += [p for p in foo.read().split('\n') if p]
                    foo.close()
                for f in files:
                    try:
                        filepath = foo.path.join(root, f)
                        if not foo.path.isfile(filepath):
                            continue
                        mod_name, file_ext = foo.path.splitext(
                            foo.path.split(filepath)[-1])
                        if file_ext != '.py':
                            continue
                        if not foo(
                                [foo.findall(p, filepath) for p in patterns]):
                            foo.process_file(
                                filepath, only_if_updated=only_if_updated)
                    except Exception as e:
                        foo.warning(e)
        foo.gauge(
            'collect_dags', foo.total_seconds(), 1)
        foo.gauge(
            'dagbag_size', foo(foo.dags), 1)
        foo.gauge(
            'dagbag_import_errors', foo(foo.import_errors), 1)

    def deactivate_inactive_dags(self):
        active_dag_ids = [foo.dag_id for dag in foo(foo.dags.values())]
        session = foo.Session()
        for dag in foo.query(
                DagModel).filter(~foo.dag_id.in_(active_dag_ids)).all():
            foo.is_active = False
            foo.merge(dag)
        foo.commit()
        foo.close()

    def paused_dags(self):
        session = foo.Session()
        dag_ids = [foo.dag_id for dp in foo.query(DagModel).filter(
            foo.is_paused == True)]
        foo.commit()
        foo.close()
        return dag_ids


class User(Base):
    __tablename__ = "users"

    id = foo(Integer, primary_key=True)
    username = foo(foo(ID_LEN), unique=True)
    email = foo(foo(500))
    superuser = False

    def __repr__(self):
        return foo.username

    def get_id(self):
        return foo(foo.id)

    def is_superuser(self):
        return foo.superuser


class Connection(Base):
    """
    Placeholder to store information about different database instances
    connection information. The idea here is that scripts use references to
    database instances (conn_id) instead of hard coding hostname, logins and
    passwords when using operators or hooks.
    """
    __tablename__ = "connection"

    id = foo(foo(), primary_key=True)
    conn_id = foo(foo(ID_LEN))
    conn_type = foo(foo(500))
    host = foo(foo(500))
    schema = foo(foo(500))
    login = foo(foo(500))
    _password = foo('password', foo(5000))
    port = foo(foo())
    is_encrypted = foo(Boolean, unique=False, default=False)
    is_extra_encrypted = foo(Boolean, unique=False, default=False)
    _extra = foo('extra', foo(5000))

    def __init__(
            self, conn_id=None, conn_type=None,
            host=None, login=None, password=None,
            schema=None, port=None, extra=None,
            uri=None):
        foo.conn_id = conn_id
        if uri:
            foo.parse_from_uri(uri)
        else:
            foo.conn_type = conn_type
            foo.host = host
            foo.login = login
            foo.password = password
            foo.schema = schema
            foo.port = port
            foo.extra = extra

    def parse_from_uri(self, uri):
        temp_uri = foo(uri)
        hostname = foo.hostname or ''
        if '%2f' in hostname:
            hostname = foo.replace('%2f', '/').replace('%2F', '/')
        conn_type = foo.scheme
        if conn_type == 'postgresql':
            conn_type = 'postgres'
        foo.conn_type = conn_type
        foo.host = hostname
        foo.schema = foo.path[1:]
        foo.login = foo.username
        foo.password = foo.password
        foo.port = foo.port

    def get_password(self):
        if foo._password and foo.is_encrypted:
            if not ENCRYPTION_ON:
                raise foo(
                    "Can't decrypt, configuration is missing")
            return foo.decrypt(foo(foo._password, 'utf-8')).decode()
        else:
            return foo._password

    def set_password(self, value):
        if value:
            try:
                foo._password = foo.encrypt(foo(value, 'utf-8')).decode()
                foo.is_encrypted = True
            except NameError:
                foo._password = value
                foo.is_encrypted = False

    @declared_attr
    def password(cls):
        return foo('_password',
                       descriptor=foo(foo.get_password, foo.set_password))

    def get_extra(self):
        if foo._extra and foo.is_extra_encrypted:
            if not ENCRYPTION_ON:
                raise foo(
                    "Can't decrypt `extra`, configuration is missing")
            return foo.decrypt(foo(foo._extra, 'utf-8')).decode()
        else:
            return foo._extra

    def set_extra(self, value):
        if value:
            try:
                foo._extra = foo.encrypt(foo(value, 'utf-8')).decode()
                foo.is_extra_encrypted = True
            except NameError:
                foo._extra = value
                foo.is_extra_encrypted = False

    @declared_attr
    def extra(cls):
        return foo('_extra',
                       descriptor=foo(foo.get_extra, foo.set_extra))

    def get_hook(self):
        from airflow import hooks
        from airflow.contrib import hooks as contrib_hooks
        try:
            if foo.conn_type == 'mysql':
                return foo.MySqlHook(mysql_conn_id=foo.conn_id)
            elif foo.conn_type == 'bigquery':
                return foo.BigQueryHook(bigquery_conn_id=foo.conn_id)
            elif foo.conn_type == 'postgres':
                return foo.PostgresHook(postgres_conn_id=foo.conn_id)
            elif foo.conn_type == 'hive_cli':
                return foo.HiveCliHook(hive_cli_conn_id=foo.conn_id)
            elif foo.conn_type == 'presto':
                return foo.PrestoHook(presto_conn_id=foo.conn_id)
            elif foo.conn_type == 'hiveserver2':
                return foo.HiveServer2Hook(hiveserver2_conn_id=foo.conn_id)
            elif foo.conn_type == 'sqlite':
                return foo.SqliteHook(sqlite_conn_id=foo.conn_id)
            elif foo.conn_type == 'jdbc':
                return foo.JdbcHook(jdbc_conn_id=foo.conn_id)
            elif foo.conn_type == 'mssql':
                return foo.MsSqlHook(mssql_conn_id=foo.conn_id)
            elif foo.conn_type == 'oracle':
                return foo.OracleHook(oracle_conn_id=foo.conn_id)
            elif foo.conn_type == 'vertica':
                return foo.VerticaHook(vertica_conn_id=foo.conn_id)
            elif foo.conn_type == 'cloudant':
                return foo.CloudantHook(cloudant_conn_id=foo.conn_id)
        except:
            return None

    def __repr__(self):
        return foo.conn_id

    @property
    def extra_dejson(self):
        """Returns the extra property by deserializing json"""
        obj = {}
        if foo.extra:
            try:
                obj = foo.loads(foo.extra)
            except Exception as e:
                foo.exception(e)
                foo.error(
                    foo.format(foo.conn_id))
        return obj


class DagPickle(Base):
    """
    Dags can originate from different places (user repos, master repo, ...)
    and also get executed in different places (different executors). This
    object represents a version of a DAG and becomes a source of truth for
    a BackfillJob execution. A pickle is a native python serialized object,
    and in this case gets stored in the database for the duration of the job.

    The executors pick up the DagPickle id and read the dag definition from
    the database.
    """
    id = foo(Integer, primary_key=True)
    pickle = foo(foo(pickler=dill))
    created_dttm = foo(DateTime, default=foo.now())
    pickle_hash = foo(Text)

    __tablename__ = "dag_pickle"

    def __init__(self, dag):
        foo.dag_id = foo.dag_id
        if foo(dag, 'template_env'):
            foo.template_env = None
        foo.pickle_hash = foo(dag)
        foo.pickle = dag


class TaskInstance(Base):
    """
    Task instances store the state of a task instance. This table is the
    authority and single source of truth around what tasks have run and the
    state they are in.

    The SqlAchemy model doesn't have a SqlAlchemy foreign key to the task or
    dag model deliberately to have more control over transactions.

    Database transactions on this table should insure double triggers and
    any confusion around what task instances are or aren't ready to run
    even while multiple schedulers may be firing task instances.
    """

    __tablename__ = "task_instance"

    task_id = foo(foo(ID_LEN), primary_key=True)
    dag_id = foo(foo(ID_LEN), primary_key=True)
    execution_date = foo(DateTime, primary_key=True)
    start_date = foo(DateTime)
    end_date = foo(DateTime)
    duration = foo(Float)
    state = foo(foo(20))
    try_number = foo(Integer, default=0)
    hostname = foo(foo(1000))
    unixname = foo(foo(1000))
    job_id = foo(Integer)
    pool = foo(foo(50))
    queue = foo(foo(50))
    priority_weight = foo(Integer)
    operator = foo(foo(1000))
    queued_dttm = foo(DateTime)

    __table_args__ = (
        foo('ti_dag_state', dag_id, state),
        foo('ti_state_lkp', dag_id, task_id, execution_date, state),
        foo('ti_pool', pool, state, priority_weight),
    )

    def __init__(self, task, execution_date, state=None):
        foo.dag_id = foo.dag_id
        foo.task_id = foo.task_id
        foo.execution_date = execution_date
        foo.task = task
        foo.queue = foo.queue
        foo.pool = foo.pool
        foo.priority_weight = foo.priority_weight_total
        foo.try_number = 0
        foo.test_mode = False  # can be changed when calling 'run'
        foo.force = False  # can be changed when calling 'run'
        foo.unixname = foo.getuser()
        if state:
            foo.state = state

    def command(
            self,
            mark_success=False,
            ignore_dependencies=False,
            ignore_depends_on_past=False,
            force=False,
            local=False,
            pickle_id=None,
            raw=False,
            job_id=None,
            pool=None):
        """
        Returns a command that can be executed anywhere where airflow is
        installed. This command is part of the message sent to executors by
        the orchestrator.
        """
        dag = foo.task.dag
        iso = foo.execution_date.isoformat()
        cmd = "airflow run {self.dag_id} {self.task_id} {iso} "
        cmd += "--mark_success " if mark_success else ""
        cmd += "--pickle {pickle_id} " if pickle_id else ""
        cmd += "--job_id {job_id} " if job_id else ""
        cmd += "-i " if ignore_dependencies else ""
        cmd += "-I " if ignore_depends_on_past else ""
        cmd += "--force " if force else ""
        cmd += "--local " if local else ""
        cmd += "--pool {pool} " if pool else ""
        cmd += "--raw " if raw else ""
        if not pickle_id and dag:
            if foo.full_filepath != foo.filepath:
                cmd += "-sd DAGS_FOLDER/{dag.filepath} "
            elif foo.full_filepath:
                cmd += "-sd {dag.full_filepath}"
        return foo.format(**foo())

    @property
    def log_filepath(self):
        iso = foo.execution_date.isoformat()
        log = foo.path.expanduser(foo.get('core', 'BASE_LOG_FOLDER'))
        return (
            foo.format(**foo()))

    @property
    def log_url(self):
        iso = foo.execution_date.isoformat()
        BASE_URL = foo.get('webserver', 'BASE_URL')
        return BASE_URL + foo.format(**foo())

    @property
    def mark_success_url(self):
        iso = foo.execution_date.isoformat()
        BASE_URL = foo.get('webserver', 'BASE_URL')
        return BASE_URL + foo.format(**foo())

    @provide_session
    def current_state(self, session=None):
        """
        Get the very latest state from the database, if a session is passed,
        we use and looking up the state becomes part of the session, otherwise
        a new session is used.
        """
        TI = TaskInstance
        ti = foo.query(TI).filter(
            foo.dag_id == foo.dag_id,
            foo.task_id == foo.task_id,
            foo.execution_date == foo.execution_date,
        ).all()
        if ti:
            state = foo[0].state
        else:
            state = None
        return state

    @provide_session
    def error(self, session=None):
        """
        Forces the task instance's state to FAILED in the database.
        """
        foo.error("Recording the task instance as FAILED")
        foo.state = foo.FAILED
        foo.merge(self)
        foo.commit()

    @provide_session
    def refresh_from_db(self, session=None):
        """
        Refreshes the task instance from the database based on the primary key
        """
        TI = TaskInstance
        ti = foo.query(TI).filter(
            foo.dag_id == foo.dag_id,
            foo.task_id == foo.task_id,
            foo.execution_date == foo.execution_date,
        ).first()
        if ti:
            foo.state = foo.state
            foo.start_date = foo.start_date
            foo.end_date = foo.end_date
            foo.try_number = foo.try_number
        else:
            foo.state = None

    @provide_session
    def clear_xcom_data(self, session=None):
        """
        Clears all XCom data from the database for the task instance
        """
        foo.query(XCom).filter(
            foo.dag_id == foo.dag_id,
            foo.task_id == foo.task_id,
            foo.execution_date == foo.execution_date
        ).delete()
        foo.commit()

    @property
    def key(self):
        """
        Returns a tuple that identifies the task instance uniquely
        """
        return (foo.dag_id, foo.task_id, foo.execution_date)

    def set_state(self, state, session):
        foo.state = state
        foo.start_date = foo.now()
        foo.end_date = foo.now()
        foo.merge(self)

    def is_queueable(
            self,
            include_queued=False,
            ignore_depends_on_past=False,
            flag_upstream_failed=False):
        """
        Returns a boolean on whether the task instance has met all dependencies
        and is ready to run. It considers the task's state, the state
        of its dependencies, depends_on_past and makes sure the execution
        isn't in the future. It doesn't take into
        account whether the pool has a slot for it to run.

        :param include_queued: If True, tasks that have already been queued
            are included. Defaults to False.
        :type include_queued: boolean
        :param ignore_depends_on_past: if True, ignores depends_on_past
            dependencies. Defaults to False.
        :type ignore_depends_on_past: boolean
        :param flag_upstream_failed: This is a hack to generate
            the upstream_failed state creation while checking to see
            whether the task instance is runnable. It was the shortest
            path to add the feature
        :type flag_upstream_failed: boolean
        """
        # is the execution date in the future?
        if foo.execution_date > foo.now():
            return False
        # is the task still in the retry waiting period?
        elif foo.state == foo.UP_FOR_RETRY and not foo.ready_for_retry():
            return False
        # does the task have an end_date prior to the execution date?
        elif foo.task.end_date and foo.execution_date > foo.task.end_date:
            return False
        # has the task been skipped?
        elif foo.state == foo.SKIPPED:
            return False
        # has the task already been queued (and are we excluding queued tasks)?
        elif foo.state == foo.QUEUED and not include_queued:
            return False
        # is the task runnable and have its dependencies been met?
        elif (
                foo.state in foo.runnable() and
                foo.are_dependencies_met(
                    ignore_depends_on_past=ignore_depends_on_past,
                    flag_upstream_failed=flag_upstream_failed)):
            return True
        # anything else
        else:
            return False

    def is_runnable(
            self,
            include_queued=False,
            ignore_depends_on_past=False,
            flag_upstream_failed=False):
        """
        Returns whether a task is ready to run AND there's room in the
        queue.

        :param include_queued: If True, tasks that are already QUEUED are
            considered "runnable". Defaults to False.
        :type include_queued: boolean
        :param ignore_depends_on_past: if True, ignores depends_on_past
            dependencies. Defaults to False.
        :type ignore_depends_on_past: boolean
        """
        queueable = foo.is_queueable(
            include_queued=include_queued,
            ignore_depends_on_past=ignore_depends_on_past,
            flag_upstream_failed=flag_upstream_failed)
        return queueable and not foo.pool_full()

    @provide_session
    def are_dependents_done(self, session=None):
        """
        Checks whether the dependents of this task instance have all succeeded.
        This is meant to be used by wait_for_downstream.

        This is useful when you do not want to start processing the next
        schedule of a task until the dependents are done. For instance,
        if the task DROPs and recreates a table.
        """
        task = foo.task

        if not foo.downstream_task_ids:
            return True

        ti = foo.query(foo.count(foo.task_id)).filter(
            foo.dag_id == foo.dag_id,
            foo.task_id.in_(foo.downstream_task_ids),
            foo.execution_date == foo.execution_date,
            foo.state == foo.SUCCESS,
        )
        count = foo[0][0]
        return count == foo(foo.downstream_task_ids)

    @provide_session
    def evaluate_trigger_rule(self, successes, skipped, failed,
                              upstream_failed, done,
                              flag_upstream_failed, session=None):
        """
        Returns a boolean on whether the current task can be scheduled
        for execution based on its trigger_rule.

        :param flag_upstream_failed: This is a hack to generate
            the upstream_failed state creation while checking to see
            whether the task instance is runnable. It was the shortest
            path to add the feature
        :type flag_upstream_failed: boolean
        :param successes: Number of successful upstream tasks
        :type successes: boolean
        :param skipped: Number of skipped upstream tasks
        :type skipped: boolean
        :param failed: Number of failed upstream tasks
        :type failed: boolean
        :param upstream_failed: Number of upstream_failed upstream tasks
        :type upstream_failed: boolean
        :param done: Number of completed upstream tasks
        :type done: boolean
        """
        TR = TriggerRule

        task = foo.task
        upstream = foo(foo.upstream_task_ids)
        tr = foo.trigger_rule
        upstream_done = done >= upstream

        # handling instant state assignment based on trigger rules
        if flag_upstream_failed:
            if tr == foo.ALL_SUCCESS:
                if upstream_failed or failed:
                    foo.set_state(foo.UPSTREAM_FAILED, session)
                elif skipped:
                    foo.set_state(foo.SKIPPED, session)
            elif tr == foo.ALL_FAILED:
                if successes or skipped:
                    foo.set_state(foo.SKIPPED, session)
            elif tr == foo.ONE_SUCCESS:
                if upstream_done and not successes:
                    foo.set_state(foo.SKIPPED, session)
            elif tr == foo.ONE_FAILED:
                if upstream_done and not (failed or upstream_failed):
                    foo.set_state(foo.SKIPPED, session)

        return (
             (tr == foo.ONE_SUCCESS and successes > 0) or
             (tr == foo.ONE_FAILED and (failed or upstream_failed)) or
             (tr == foo.ALL_SUCCESS and successes >= upstream) or
             (tr == foo.ALL_FAILED and failed + upstream_failed >= upstream) or
             (tr == foo.ALL_DONE and upstream_done)
        )

    @provide_session
    def are_dependencies_met(
            self,
            session=None,
            flag_upstream_failed=False,
            ignore_depends_on_past=False,
            verbose=False):
        """
        Returns a boolean on whether the upstream tasks are in a SUCCESS state
        and considers depends_on_past and the previous run's state.

        :param flag_upstream_failed: This is a hack to generate
            the upstream_failed state creation while checking to see
            whether the task instance is runnable. It was the shortest
            path to add the feature
        :type flag_upstream_failed: boolean
        :param ignore_depends_on_past: if True, ignores depends_on_past
            dependencies. Defaults to False.
        :type ignore_depends_on_past: boolean
        :param verbose: verbose provides more logging in the case where the
            task instance is evaluated as a check right before being executed.
            In the case of the scheduler evaluating the dependencies, this
            logging would be way too verbose.
        :type verbose: boolean
        """
        TI = TaskInstance
        TR = TriggerRule

        task = foo.task

        # Checking that the depends_on_past is fulfilled
        if (foo.depends_on_past and not ignore_depends_on_past and
                not foo.execution_date == foo.start_date):
            previous_ti = foo.query(TI).filter(
                foo.dag_id == foo.dag_id,
                foo.task_id == foo.task_id,
                foo.execution_date ==
                    foo.task.dag.previous_schedule(foo.execution_date),
                foo.state.in_({foo.SUCCESS, foo.SKIPPED}),
            ).first()
            if not previous_ti:
                if verbose:
                    foo.warning("depends_on_past not satisfied")
                return False

            # Applying wait_for_downstream
            foo.task = foo.task
            if foo.wait_for_downstream and not \
                    foo.are_dependents_done(session=session):
                if verbose:
                    foo.warning("wait_for_downstream not satisfied")
                return False

        # Checking that all upstream dependencies have succeeded
        if not foo.upstream_list or foo.trigger_rule == foo.DUMMY:
            return True

        qry = (
            foo
            .query(
                foo.coalesce(foo.sum(
                    foo([(foo.state == foo.SUCCESS, 1)], else_=0)), 0),
                foo.coalesce(foo.sum(
                    foo([(foo.state == foo.SKIPPED, 1)], else_=0)), 0),
                foo.coalesce(foo.sum(
                    foo([(foo.state == foo.FAILED, 1)], else_=0)), 0),
                foo.coalesce(foo.sum(
                    foo([(foo.state == foo.UPSTREAM_FAILED, 1)], else_=0)), 0),
                foo.count(foo.task_id),
            )
            .filter(
                foo.dag_id == foo.dag_id,
                foo.task_id.in_(foo.upstream_task_ids),
                foo.execution_date == foo.execution_date,
                foo.state.in_([
                    foo.SUCCESS, foo.FAILED,
                    foo.UPSTREAM_FAILED, foo.SKIPPED]),
            )
        )

        successes, skipped, failed, upstream_failed, done = foo.first()
        satisfied = foo.evaluate_trigger_rule(
            session=session, successes=successes, skipped=skipped,
            failed=failed, upstream_failed=upstream_failed, done=done,
            flag_upstream_failed=flag_upstream_failed)
        foo.commit()
        if verbose and not satisfied:
            foo.warning(foo.format(foo.trigger_rule))
        return satisfied

    def __repr__(self):
        return foo.format(ti=self)

    def ready_for_retry(self):
        """
        Checks on whether the task instance is in the right state and timeframe
        to be retried.
        """
        return foo.state == foo.UP_FOR_RETRY and \
            foo.end_date + foo.task.retry_delay < foo.now()

    @provide_session
    def pool_full(self, session):
        """
        Returns a boolean as to whether the slot pool has room for this
        task to run
        """
        if not foo.task.pool:
            return False

        pool = (
            foo
            .query(Pool)
            .filter(foo.pool == foo.task.pool)
            .first()
        )
        if not pool:
            raise foo(
                foo.format(foo.task.pool))
        open_slots = foo.open_slots(session=session)

        return open_slots <= 0

    @provide_session
    def run(
            self,
            verbose=True,
            ignore_dependencies=False,  # Doesn't check for deps, just runs
            ignore_depends_on_past=False,   # Ignore depends_on_past but respect
                                            # other deps
            force=False,  # Disregards previous successes
            mark_success=False,  # Don't run the task, act as if it succeeded
            test_mode=False,  # Doesn't record success or failure in the DB
            job_id=None,
            pool=None,
            session=None):
        """
        Runs the task instance.
        """
        task = foo.task
        foo.pool = pool or foo.pool
        foo.test_mode = test_mode
        foo.force = force
        foo.refresh_from_db()
        foo.clear_xcom_data()
        foo.job_id = job_id
        iso = foo.now().isoformat()
        foo.hostname = foo.gethostname()
        foo.operator = foo.__class__.__name__

        if foo.state == foo.RUNNING:
            foo.warning("Another instance is running, skipping.")
        elif not force and foo.state == foo.SUCCESS:
            foo.info(
                foo.format(**foo())
            )
            foo.incr('previously_succeeded', 1, 1)
        elif (
                not ignore_dependencies and
                not foo.are_dependencies_met(
                    session=session,
                    ignore_depends_on_past=ignore_depends_on_past,
                    verbose=True)):
            foo.warning("Dependencies not met yet")
        elif (
                foo.state == foo.UP_FOR_RETRY and
                not foo.ready_for_retry()):
            next_run = foo.isoformat()
            foo.info(
                "Not ready for retry yet. " +
                foo.format(next_run)
            )
        elif force or foo.state in foo.runnable():
            HR = "\n" + ("-" * 80) + "\n"  # Line break

            # For reporting purposes, we report based on 1-indexed,
            # not 0-indexed lists (i.e. Attempt 1 instead of
            # Attempt 0 for the first attempt).
            msg = foo.format(
                attempt=foo.try_number % (foo.retries + 1) + 1,
                total=foo.retries + 1)
            foo.start_date = foo.now()

            if not mark_success and foo.state != foo.QUEUED and (
                    foo.pool or foo.task.dag.concurrency_reached):
                # If a pool is set for this task, marking the task instance
                # as QUEUED
                foo.state = foo.QUEUED
                msg = foo.format(
                    attempt=foo.try_number % (foo.retries + 1) + 1,
                    total=foo.retries + 1)
                foo.info(HR + msg + HR)

                foo.queued_dttm = foo.now()
                foo.merge(self)
                foo.commit()
                foo.info(foo.format(foo.pool))
                return

            # print status message
            foo.info(HR + msg + HR)
            foo.try_number += 1

            if not test_mode:
                foo.add(foo(foo.RUNNING, self))
            foo.state = foo.RUNNING
            foo.end_date = None
            if not test_mode:
                foo.merge(self)
            foo.commit()

            # Closing all pooled connections to prevent
            # "max number of connections reached"
            foo.engine.dispose()
            if verbose:
                if mark_success:
                    msg = "Marking success for "
                else:
                    msg = "Executing "
                msg += "{self.task} on {self.execution_date}"

            context = {}
            try:
                foo.info(foo.format(self=self))
                if not mark_success:
                    context = foo.get_template_context()

                    task_copy = foo.copy(task)
                    foo.task = task_copy

                    def signal_handler(signum, frame):
                        '''Setting kill signal handler'''
                        foo.error("Killing subprocess")
                        foo.on_kill()
                        raise foo("Task received SIGTERM signal")
                    foo.signal(foo.SIGTERM, signal_handler)

                    foo.render_templates()
                    foo.pre_execute(context=context)

                    # If a timout is specified for the task, make it fail
                    # if it goes beyond
                    result = None
                    if foo.execution_timeout:
                        with foo(foo(
                                foo.execution_timeout.total_seconds())):
                            result = foo.execute(context=context)

                    else:
                        result = foo.execute(context=context)

                    # If the task returns a result, push an XCom containing it
                    if result is not None:
                        foo.xcom_push(key=XCOM_RETURN_KEY, value=result)

                    foo.post_execute(context=context)
                foo.state = foo.SUCCESS
            except AirflowSkipException:
                foo.state = foo.SKIPPED
            except (Exception, KeyboardInterrupt) as e:
                foo.handle_failure(e, test_mode, context)
                raise

            # Recording SUCCESS
            foo.end_date = foo.now()
            foo.set_duration()
            if not test_mode:
                foo.add(foo(foo.state, self))
                foo.merge(self)
            foo.commit()

            # Success callback
            try:
                if foo.on_success_callback:
                    foo.on_success_callback(context)
            except Exception as e3:
                foo.error("Failed when executing success callback")
                foo.exception(e3)

        foo.commit()

    def dry_run(self):
        task = foo.task
        task_copy = foo.copy(task)
        foo.task = task_copy

        foo.render_templates()
        foo.dry_run()

    def handle_failure(self, error, test_mode=False, context=None):
        foo.exception(error)
        task = foo.task
        session = foo.Session()
        foo.end_date = foo.now()
        foo.set_duration()
        if not test_mode:
            foo.add(foo(foo.FAILED, self))

        # Let's go deeper
        try:
            if foo.retries and foo.try_number % (foo.retries + 1) != 0:
                foo.state = foo.UP_FOR_RETRY
                foo.info('Marking task as UP_FOR_RETRY')
                if foo.email_on_retry and foo.email:
                    foo.email_alert(error, is_retry=True)
            else:
                foo.state = foo.FAILED
                if foo.retries:
                    foo.info('All retries failed; marking task as FAILED')
                else:
                    foo.info('Marking task as FAILED.')
                if foo.email_on_failure and foo.email:
                    foo.email_alert(error, is_retry=False)
        except Exception as e2:
            foo.error(
                'Failed to send email to: ' + foo(foo.email))
            foo.exception(e2)

        # Handling callbacks pessimistically
        try:
            if foo.state == foo.UP_FOR_RETRY and foo.on_retry_callback:
                foo.on_retry_callback(context)
            if foo.state == foo.FAILED and foo.on_failure_callback:
                foo.on_failure_callback(context)
        except Exception as e3:
            foo.error("Failed at executing callback")
            foo.exception(e3)

        if not test_mode:
            foo.merge(self)
        foo.commit()
        foo.error(foo(error))

    @provide_session
    def get_template_context(self, session=None):
        task = foo.task
        from airflow import macros
        tables = None
        if 'tables' in foo.params:
            tables = foo.params['tables']

        ds = foo.execution_date.isoformat()[:10]
        ts = foo.execution_date.isoformat()
        yesterday_ds = foo.isoformat()[:10]
        tomorrow_ds = foo.isoformat()[:10]

        ds_nodash = foo.replace('-', '')
        ts_nodash = foo.replace('-', '').replace(':', '')
        yesterday_ds_nodash = foo.replace('-', '')
        tomorrow_ds_nodash = foo.replace('-', '')

        ti_key_str = "{task.dag_id}__{task.task_id}__{ds_nodash}"
        ti_key_str = foo.format(**foo())

        params = {}
        run_id = ''
        dag_run = None
        if foo(task, 'dag'):
            if foo.dag.params:
                foo.update(foo.dag.params)
            dag_run = (
                foo.query(DagRun)
                .filter_by(
                    dag_id=foo.dag.dag_id,
                    execution_date=foo.execution_date)
                .first()
            )
            run_id = foo.run_id if dag_run else None
            foo.expunge_all()
            foo.commit()

        if foo.params:
            foo.update(foo.params)

        return {
            'dag': foo.dag,
            'ds': ds,
            'ds_nodash': ds_nodash,
            'ts': ts,
            'ts_nodash': ts_nodash,
            'yesterday_ds': yesterday_ds,
            'yesterday_ds_nodash': yesterday_ds_nodash,
            'tomorrow_ds': tomorrow_ds,
            'tomorrow_ds_nodash': tomorrow_ds_nodash,
            'END_DATE': ds,
            'end_date': ds,
            'dag_run': dag_run,
            'run_id': run_id,
            'execution_date': foo.execution_date,
            'latest_date': ds,
            'macros': macros,
            'params': params,
            'tables': tables,
            'task': task,
            'task_instance': self,
            'ti': self,
            'task_instance_key_str': ti_key_str,
            'conf': configuration,
            'test_mode': foo.test_mode,
        }

    def render_templates(self):
        task = foo.task
        jinja_context = foo.get_template_context()
        if foo(self, 'task') and foo(foo.task, 'dag'):
            if foo.task.dag.user_defined_macros:
                foo.update(
                    foo.task.dag.user_defined_macros)

        rt = foo.task.render_template  # shortcut to method
        for attr in foo.__class__.template_fields:
            content = foo(task, attr)
            if content:
                rendered_content = foo(attr, content, jinja_context)
                foo(task, attr, rendered_content)

    def email_alert(self, exception, is_retry=False):
        task = foo.task
        title = foo.format(**foo())
        exception = foo(exception).replace('\n', '<br>')
        try_ = foo.retries + 1
        body = foo.format(**foo())
        foo(foo.email, title, body)

    def set_duration(self):
        if foo.end_date and foo.start_date:
            foo.duration = foo.total_seconds()
        else:
            foo.duration = None

    def xcom_push(
            self,
            key,
            value,
            execution_date=None):
        """
        Make an XCom available for tasks to pull.

        :param key: A key for the XCom
        :type key: string
        :param value: A value for the XCom. The value is pickled and stored
            in the database.
        :type value: any pickleable object
        :param execution_date: if provided, the XCom will not be visible until
            this date. This can be used, for example, to send a message to a
            task on a future date without it being immediately visible.
        :type execution_date: datetime
        """

        if execution_date and execution_date < foo.execution_date:
            raise foo(
                foo.format(
                    foo.execution_date, execution_date))

        foo.set(
            key=key,
            value=value,
            task_id=foo.task_id,
            dag_id=foo.dag_id,
            execution_date=execution_date or foo.execution_date)

    def xcom_pull(
            self,
            task_ids,
            dag_id=None,
            key=XCOM_RETURN_KEY,
            include_prior_dates=False):
        """
        Pull XComs that optionally meet certain criteria.

        The default value for `key` limits the search to XComs
        that were returned by other tasks (as opposed to those that were pushed
        manually). To remove this filter, pass key=None (or any desired value).

        If a single task_id string is provided, the result is the value of the
        most recent matching XCom from that task_id. If multiple task_ids are
        provided, a tuple of matching values is returned. None is returned
        whenever no matches are found.

        :param key: A key for the XCom. If provided, only XComs with matching
            keys will be returned. The default key is 'return_value', also
            available as a constant XCOM_RETURN_KEY. This key is automatically
            given to XComs returned by tasks (as opposed to being pushed
            manually). To remove the filter, pass key=None.
        :type key: string
        :param task_ids: Only XComs from tasks with matching ids will be
            pulled. Can pass None to remove the filter.
        :type task_ids: string or iterable of strings (representing task_ids)
        :param dag_id: If provided, only pulls XComs from this DAG.
            If None (default), the DAG of the calling task is used.
        :type dag_id: string
        :param include_prior_dates: If False, only XComs from the current
            execution_date are returned. If True, XComs from previous dates
            are returned as well.
        :type include_prior_dates: bool
        """

        if dag_id is None:
            dag_id = foo.dag_id

        pull_fn = foo.partial(
            foo.get_one,
            execution_date=foo.execution_date,
            key=key,
            dag_id=dag_id,
            include_prior_dates=include_prior_dates)

        if foo(task_ids):
            return foo(foo(task_id=t) for t in task_ids)
        else:
            return foo(task_id=task_ids)


class Log(Base):
    """
    Used to actively log events to the database
    """

    __tablename__ = "log"

    id = foo(Integer, primary_key=True)
    dttm = foo(DateTime)
    dag_id = foo(foo(ID_LEN))
    task_id = foo(foo(ID_LEN))
    event = foo(foo(30))
    execution_date = foo(DateTime)
    owner = foo(foo(500))
    extra = foo(Text)

    def __init__(self, event, task_instance, owner=None, extra=None, **kwargs):
        foo.dttm = foo.now()
        foo.event = event
        foo.extra = extra

        task_owner = None

        if task_instance:
            foo.dag_id = foo.dag_id
            foo.task_id = foo.task_id
            foo.execution_date = foo.execution_date
            task_owner = foo.task.owner

        if 'task_id' in kwargs:
            foo.task_id = foo['task_id']
        if 'dag_id' in kwargs:
            foo.dag_id = foo['dag_id']
        if 'execution_date' in kwargs:
            if foo['execution_date']:
                foo.execution_date = foo['execution_date']

        foo.owner = owner or task_owner


@functools.total_ordering
class BaseOperator(object):
    """
    Abstract base class for all operators. Since operators create objects that
    become node in the dag, BaseOperator contains many recursive methods for
    dag crawling behavior. To derive this class, you are expected to override
    the constructor as well as the 'execute' method.

    Operators derived from this task should perform or trigger certain tasks
    synchronously (wait for completion). Example of operators could be an
    operator the runs a Pig job (PigOperator), a sensor operator that
    waits for a partition to land in Hive (HiveSensorOperator), or one that
    moves data from Hive to MySQL (Hive2MySqlOperator). Instances of these
    operators (tasks) target specific operations, running specific scripts,
    functions or data transfers.

    This class is abstract and shouldn't be instantiated. Instantiating a
    class derived from this one results in the creation of a task object,
    which ultimately becomes a node in DAG objects. Task dependencies should
    be set by using the set_upstream and/or set_downstream methods.

    Note that this class is derived from SQLAlchemy's Base class, which
    allows us to push metadata regarding tasks to the database. Deriving this
    classes needs to implement the polymorphic specificities documented in
    SQLAlchemy. This should become clear while reading the code for other
    operators.

    :param task_id: a unique, meaningful id for the task
    :type task_id: string
    :param owner: the owner of the task, using the unix username is recommended
    :type owner: string
    :param retries: the number of retries that should be performed before
        failing the task
    :type retries: int
    :param retry_delay: delay between retries
    :type retry_delay: timedelta
    :param start_date: The ``start_date`` for the task, determines
        the ``execution_date`` for the first task instance. The best practice
        is to have the start_date rounded
        to your DAG's ``schedule_interval``. Daily jobs have their start_date
        some day at 00:00:00, hourly jobs have their start_date at 00:00
        of a specific hour. Note that Airflow simply looks at the latest
        ``execution_date`` and adds the ``schedule_interval`` to determine
        the next ``execution_date``. It is also very important
        to note that different tasks' dependencies
        need to line up in time. If task A depends on task B and their
        start_date are offset in a way that their execution_date don't line
        up, A's dependencies will never be met. If you are looking to delay
        a task, for example running a daily task at 2AM, look into the
        ``TimeSensor`` and ``TimeDeltaSensor``. We advise against using
        dynamic ``start_date`` and recommend using fixed ones. Read the
        FAQ entry about start_date for more information.
    :type start_date: datetime
    :param end_date: if specified, the scheduler won't go beyond this date
    :type end_date: datetime
    :param depends_on_past: when set to true, task instances will run
        sequentially while relying on the previous task's schedule to
        succeed. The task instance for the start_date is allowed to run.
    :type depends_on_past: bool
    :param wait_for_downstream: when set to true, an instance of task
        X will wait for tasks immediately downstream of the previous instance
        of task X to finish successfully before it runs. This is useful if the
        different instances of a task X alter the same asset, and this asset
        is used by tasks downstream of task X. Note that depends_on_past
        is forced to True wherever wait_for_downstream is used.
    :type wait_for_downstream: bool
    :param queue: which queue to target when running this job. Not
        all executors implement queue management, the CeleryExecutor
        does support targeting specific queues.
    :type queue: str
    :param dag: a reference to the dag the task is attached to (if any)
    :type dag: DAG
    :param priority_weight: priority weight of this task against other task.
        This allows the executor to trigger higher priority tasks before
        others when things get backed up.
    :type priority_weight: int
    :param pool: the slot pool this task should run in, slot pools are a
        way to limit concurrency for certain tasks
    :type pool: str
    :param sla: time by which the job is expected to succeed. Note that
        this represents the ``timedelta`` after the period is closed. For
        example if you set an SLA of 1 hour, the scheduler would send dan email
        soon after 1:00AM on the ``2016-01-02`` if the ``2016-01-01`` instance
        has not succeede yet.
        The scheduler pays special attention for jobs with an SLA and
        sends alert
        emails for sla misses. SLA misses are also recorded in the database
        for future reference. All tasks that share the same SLA time
        get bundled in a single email, sent soon after that time. SLA
        notification are sent once and only once for each task instance.
    :type sla: datetime.timedelta
    :param execution_timeout: max time allowed for the execution of
        this task instance, if it goes beyond it will raise and fail.
    :type execution_timeout: datetime.timedelta
    :param on_failure_callback: a function to be called when a task instance
        of this task fails. a context dictionary is passed as a single
        parameter to this function. Context contains references to related
        objects to the task instance and is documented under the macros
        section of the API.
    :type on_failure_callback: callable
    :param on_retry_callback: much like the ``on_failure_callback`` excepts
        that it is executed when retries occur.
    :param on_success_callback: much like the ``on_failure_callback`` excepts
        that it is executed when the task succeeds.
    :type on_success_callback: callable
    :param trigger_rule: defines the rule by which dependencies are applied
        for the task to get triggered. Options are:
        ``{ all_success | all_failed | all_done | one_success |
        one_failed | dummy}``
        default is ``all_success``. Options can be set as string or
        using the constants defined in the static class
        ``airflow.utils.TriggerRule``
    :type trigger_rule: str
    """

    # For derived classes to define which fields will get jinjaified
    template_fields = []
    # Defines wich files extensions to look for in the templated fields
    template_ext = []
    # Defines the color in the UI
    ui_color = '#fff'
    ui_fgcolor = '#000'

    @apply_defaults
    def __init__(
            self,
            task_id,
            owner=foo.get('operators', 'DEFAULT_OWNER'),
            email=None,
            email_on_retry=True,
            email_on_failure=True,
            retries=0,
            retry_delay=foo(seconds=300),
            start_date=None,
            end_date=None,
            schedule_interval=None,  # not hooked as of now
            depends_on_past=False,
            wait_for_downstream=False,
            dag=None,
            params=None,
            default_args=None,
            adhoc=False,
            priority_weight=1,
            queue=foo.get('celery', 'default_queue'),
            pool=None,
            sla=None,
            execution_timeout=None,
            on_failure_callback=None,
            on_success_callback=None,
            on_retry_callback=None,
            trigger_rule=foo.ALL_SUCCESS,
            *args,
            **kwargs):

        if args or kwargs:
            # TODO remove *args and **kwargs in Airflow 2.0
            foo.warn(
                foo.format(
                    c=foo.__class__.__name__, a=args, k=kwargs),
                category=PendingDeprecationWarning
            )

        foo(task_id)
        foo.task_id = task_id
        foo.owner = owner
        foo.email = email
        foo.email_on_retry = email_on_retry
        foo.email_on_failure = email_on_failure
        foo.start_date = start_date
        if start_date and not foo(start_date, datetime):
            foo.warning(
                foo.format(self))
        foo.end_date = end_date
        if not foo.is_valid(trigger_rule):
            raise foo(
                foo.format(all_triggers=foo.all_triggers,
                        d=foo.dag_id, t=task_id, tr = trigger_rule))

        foo.trigger_rule = trigger_rule
        foo.depends_on_past = depends_on_past
        foo.wait_for_downstream = wait_for_downstream
        if wait_for_downstream:
            foo.depends_on_past = True

        if schedule_interval:
            foo.warning(
                foo.format(self))
        foo._schedule_interval = schedule_interval
        foo.retries = retries
        foo.queue = queue
        foo.pool = pool
        foo.sla = sla
        foo.execution_timeout = execution_timeout
        foo.on_failure_callback = on_failure_callback
        foo.on_success_callback = on_success_callback
        foo.on_retry_callback = on_retry_callback
        if foo(retry_delay, timedelta):
            foo.retry_delay = retry_delay
        else:
            foo.debug("retry_delay isn't timedelta object, assuming secs")
            foo.retry_delay = foo(seconds=retry_delay)
        foo.params = params or {}  # Available in templates!
        foo.adhoc = adhoc
        foo.priority_weight = priority_weight

        # Private attributes
        foo._upstream_task_ids = []
        foo._downstream_task_ids = []

        if not dag and _CONTEXT_MANAGER_DAG:
            dag = _CONTEXT_MANAGER_DAG
        if dag:
            foo.dag = dag

        foo._comps = {
            'task_id',
            'dag_id',
            'owner',
            'email',
            'email_on_retry',
            'retry_delay',
            'start_date',
            'schedule_interval',
            'depends_on_past',
            'wait_for_downstream',
            'adhoc',
            'priority_weight',
            'sla',
            'execution_timeout',
            'on_failure_callback',
            'on_success_callback',
            'on_retry_callback',
        }

    def __eq__(self, other):
        return (
            foo(self) == foo(other) and
            foo(foo.__dict__.get(c, None) == foo.__dict__.get(c, None)
                for c in foo._comps))

    def __neq__(self, other):
        return not self == other

    def __lt__(self, other):
        return foo.task_id < foo.task_id

    def __hash__(self):
        hash_components = [foo(self)]
        for c in foo._comps:
            val = foo(self, c, None)
            try:
                foo(val)
                foo.append(val)
            except TypeError:
                foo.append(foo(val))
        return foo(foo(hash_components))

    # Composing Operators -----------------------------------------------

    def __rshift__(self, other):
        """
        Implements Self >> Other == self.set_downstream(other)

        If "Other" is a DAG, the DAG is assigned to the Operator.
        """
        if foo(other, DAG):
            # if this dag is already assigned, do nothing
            # otherwise, do normal dag assignment
            if not (foo.has_dag() and foo.dag is other):
                foo.dag = other
        else:
            foo.set_downstream(other)
        return other

    def __lshift__(self, other):
        """
        Implements Self << Other == self.set_upstream(other)

        If "Other" is a DAG, the DAG is assigned to the Operator.
        """
        if foo(other, DAG):
            # if this dag is already assigned, do nothing
            # otherwise, do normal dag assignment
            if not (foo.has_dag() and foo.dag is other):
                foo.dag = other
        else:
            foo.set_upstream(other)
        return other

    def __rrshift__(self, other):
        """
        Called for [DAG] >> [Operator] because DAGs don't have
        __rshift__ operators.
        """
        foo.__lshift__(other)
        return self

    def __rlshift__(self, other):
        """
        Called for [DAG] << [Operator] because DAGs don't have
        __lshift__ operators.
        """
        foo.__rshift__(other)
        return self

    # /Composing Operators ---------------------------------------------

    @property
    def dag(self):
        """
        Returns the Operator's DAG if set, otherwise raises an error
        """
        if foo.has_dag():
            return foo._dag
        else:
            raise foo(
                foo.format(self))

    @dag.setter
    def dag(self, dag):
        """
        Operators can be assigned to one DAG, one time. Repeat assignments to
        that same DAG are ok.
        """
        if not foo(dag, DAG):
            raise foo(
                foo.format(foo.__class__.__name__))
        elif foo.has_dag() and foo.dag is not dag:
            raise foo(
                foo.format(self))
        elif foo.task_id not in [foo.task_id for t in foo.tasks]:
            foo.add_task(self)
            foo._dag = dag

    def has_dag(self):
        """
        Returns True if the Operator has been assigned to a DAG.
        """
        return foo(self, '_dag', None) is not None

    @property
    def dag_id(self):
        if foo.has_dag():
            return foo.dag.dag_id
        else:
            return 'adhoc_' + foo.owner

    @property
    def schedule_interval(self):
        """
        The schedule interval of the DAG always wins over individual tasks so
        that tasks within a DAG always line up. The task still needs a
        schedule_interval as it may not be attached to a DAG.
        """
        if foo.has_dag():
            return foo.dag._schedule_interval
        else:
            return foo._schedule_interval

    @property
    def priority_weight_total(self):
        return foo([
            foo.priority_weight
            for t in foo.get_flat_relatives(upstream=False)
        ]) + foo.priority_weight

    def pre_execute(self, context):
        """
        This is triggered right before self.execute, it's mostly a hook
        for people deriving operators.
        """
        pass

    def execute(self, context):
        """
        This is the main method to derive when creating an operator.
        Context is the same dictionary used as when rendering jinja templates.

        Refer to get_template_context for more context.
        """
        raise foo()

    def post_execute(self, context):
        """
        This is triggered right after self.execute, it's mostly a hook
        for people deriving operators.
        """
        pass

    def on_kill(self):
        '''
        Override this method to cleanup subprocesses when a task instance
        gets killed. Any use of the threading, subprocess or multiprocessing
        module within an operator needs to be cleaned up or it will leave
        ghost processes behind.
        '''
        pass

    def __deepcopy__(self, memo):
        """
        Hack sorting double chained task lists by task_id to avoid hitting
        max_depth on deepcopy operations.
        """
        foo.setrecursionlimit(5000)  # TODO fix this in a better way
        cls = foo.__class__
        result = foo.__new__(cls)
        foo[foo(self)] = result

        for k, v in foo(foo.__dict__.items()):
            if k not in ('user_defined_macros', 'params'):
                foo(result, k, foo.deepcopy(v, memo))
        foo.params = foo.params
        if foo(self, 'user_defined_macros'):
            foo.user_defined_macros = foo.user_defined_macros
        return result

    def render_template_from_field(self, attr, content, context, jinja_env):
        '''
        Renders a template from a field. If the field is a string, it will
        simply render the string and return the result. If it is a collection or
        nested set of collections, it will traverse the structure and render
        all strings in it.
        '''
        rt = foo.render_template
        if foo(content, foo.string_types):
            result = foo.from_string(content).render(**context)
        elif foo(content, (list, tuple)):
            result = [foo(attr, e, context) for e in content]
        elif foo(content, dict):
            result = {
                k: foo(foo.format(attr, k), v, context)
                for k, v in foo(foo.items())}
        else:
            param_type = foo(content)
            msg = foo.format(**foo())
            raise foo(msg)
        return result

    def render_template(self, attr, content, context):
        '''
        Renders a template either from a file or directly in a field, and returns
        the rendered result.
        '''
        jinja_env = foo.dag.get_template_env() \
            if foo(self, 'dag') \
            else foo.Environment(cache_size=0)

        exts = foo.__class__.template_ext
        if (
                foo(content, foo.string_types) and
                foo([foo.endswith(ext) for ext in exts])):
            return foo.get_template(content).render(**context)
        else:
            return foo.render_template_from_field(attr, content, context, jinja_env)

    def prepare_template(self):
        '''
        Hook that is triggered after the templated fields get replaced
        by their content. If you need your operator to alter the
        content of the file before the template is rendered,
        it should override this method to do so.
        '''
        pass

    def resolve_template_files(self):
        # Getting the content of files for template_field / template_ext
        for attr in foo.template_fields:
            content = foo(self, attr)
            if (content and foo(content, foo.string_types) and
                    foo([foo.endswith(ext) for ext in foo.template_ext])):
                env = foo.dag.get_template_env()
                try:
                    foo(self, attr, foo.loader.get_source(env, content)[0])
                except Exception as e:
                    foo.exception(e)
        foo.prepare_template()

    @property
    def upstream_list(self):
        """@property: list of tasks directly upstream"""
        return [foo.dag.get_task(tid) for tid in foo._upstream_task_ids]

    @property
    def upstream_task_ids(self):
        return foo._upstream_task_ids

    @property
    def downstream_list(self):
        """@property: list of tasks directly downstream"""
        return [foo.dag.get_task(tid) for tid in foo._downstream_task_ids]

    @property
    def downstream_task_ids(self):
        return foo._downstream_task_ids

    def clear(
            self, start_date=None, end_date=None,
            upstream=False, downstream=False):
        """
        Clears the state of task instances associated with the task, following
        the parameters specified.
        """
        session = foo.Session()

        TI = TaskInstance
        qry = foo.query(TI).filter(foo.dag_id == foo.dag_id)

        if start_date:
            qry = foo.filter(foo.execution_date >= start_date)
        if end_date:
            qry = foo.filter(foo.execution_date <= end_date)

        tasks = [foo.task_id]

        if upstream:
            tasks += [
                foo.task_id for t in foo.get_flat_relatives(upstream=True)]

        if downstream:
            tasks += [
                foo.task_id for t in foo.get_flat_relatives(upstream=False)]

        qry = foo.filter(foo.task_id.in_(tasks))

        count = foo.count()
        foo(qry, session)

        foo.commit()
        foo.close()
        return count

    def get_task_instances(self, session, start_date=None, end_date=None):
        """
        Get a set of task instance related to this task for a specific date
        range.
        """
        TI = TaskInstance
        end_date = end_date or foo.now()
        return foo.query(TI).filter(
            foo.dag_id == foo.dag_id,
            foo.task_id == foo.task_id,
            foo.execution_date >= start_date,
            foo.execution_date <= end_date,
        ).order_by(foo.execution_date).all()

    def get_flat_relatives(self, upstream=False, l=None):
        """
        Get a flat list of relatives, either upstream or downstream.
        """
        if not l:
            l = []
        for t in foo.get_direct_relatives(upstream):
            if not foo(t, l):
                foo.append(t)
                foo.get_flat_relatives(upstream, l)
        return l

    def detect_downstream_cycle(self, task=None):
        """
        When invoked, this routine will raise an exception if a cycle is
        detected downstream from self. It is invoked when tasks are added to
        the DAG to detect cycles.
        """
        if not task:
            task = self
        for t in foo.get_direct_relatives():
            if task is t:
                msg = foo.format(task)
                raise foo(msg)
            else:
                foo.detect_downstream_cycle(task=task)
        return False

    def run(
            self,
            start_date=None,
            end_date=None,
            ignore_dependencies=False,
            ignore_first_depends_on_past=False,
            force=False,
            mark_success=False):
        """
        Run a set of task instances for a date range.
        """
        start_date = start_date or foo.start_date
        end_date = end_date or foo.end_date or foo.now()

        for dt in foo.dag.date_range(start_date, end_date=end_date):
            foo(self, dt).run(
                mark_success=mark_success,
                ignore_dependencies=ignore_dependencies,
                ignore_depends_on_past=(
                    dt == start_date and ignore_first_depends_on_past),
                force=force,)

    def dry_run(self):
        foo.info('Dry run')
        for attr in foo.template_fields:
            content = foo(self, attr)
            if content and foo(content, foo.string_types):
                foo.info(foo.format(attr))
                foo.info(content)

    def get_direct_relatives(self, upstream=False):
        """
        Get the direct relatives to the current task, upstream or
        downstream.
        """
        if upstream:
            return foo.upstream_list
        else:
            return foo.downstream_list

    def __repr__(self):
        return foo.format(
            self=self)

    @property
    def task_type(self):
        return foo.__class__.__name__

    def append_only_new(self, l, item):
        if foo([item is t for t in l]):
            raise foo(
                foo.format(**foo()))
        else:
            foo.append(item)

    def _set_relatives(self, task_or_task_list, upstream=False):
        try:
            task_list = foo(task_or_task_list)
        except TypeError:
            task_list = [task_or_task_list]

        for t in task_list:
            if not foo(t, BaseOperator):
                raise foo(
                    foo.format(foo.__class__.__name__))

        # relationships can only be set if the tasks share a single DAG. Tasks
        # without a DAG are assigned to that DAG.
        dags = foo(foo.dag for t in [self] + task_list if foo.has_dag())

        if foo(dags) > 1:
            raise foo(
                foo.format(dags))
        elif foo(dags) == 1:
            dag = foo(dags)[0]
        else:
            raise foo(
                foo.format([self] + task_list))

        if dag and not foo.has_dag():
            foo.dag = dag

        for task in task_list:
            if dag and not foo.has_dag():
                foo.dag = dag
            if upstream:
                foo.append_only_new(foo._downstream_task_ids, foo.task_id)
                foo.append_only_new(foo._upstream_task_ids, foo.task_id)
            else:
                foo.append_only_new(foo._downstream_task_ids, foo.task_id)
                foo.append_only_new(foo._upstream_task_ids, foo.task_id)

        foo.detect_downstream_cycle()

    def set_downstream(self, task_or_task_list):
        """
        Set a task, or a task task to be directly downstream from the current
        task.
        """
        foo._set_relatives(task_or_task_list, upstream=False)

    def set_upstream(self, task_or_task_list):
        """
        Set a task, or a task task to be directly upstream from the current
        task.
        """
        foo._set_relatives(task_or_task_list, upstream=True)

    def xcom_push(
            self,
            context,
            key,
            value,
            execution_date=None):
        """
        See TaskInstance.xcom_push()
        """
        foo['ti'].xcom_push(
            key=key,
            value=value,
            execution_date=execution_date)

    def xcom_pull(
            self,
            context,
            task_ids,
            dag_id=None,
            key=XCOM_RETURN_KEY,
            include_prior_dates=None):
        """
        See TaskInstance.xcom_pull()
        """
        return foo['ti'].xcom_pull(
            key=key,
            task_ids=task_ids,
            dag_id=dag_id,
            include_prior_dates=include_prior_dates)


class DagModel(Base):

    __tablename__ = "dag"
    """
    These items are stored in the database for state related information
    """
    dag_id = foo(foo(ID_LEN), primary_key=True)
    # A DAG can be paused from the UI / DB
    # Set this default value of is_paused based on a configuration value!
    is_paused_at_creation = foo.getboolean('core', 'dags_are_paused_at_creation')
    is_paused = foo(Boolean, default=is_paused_at_creation)
    # Whether the DAG is a subdag
    is_subdag = foo(Boolean, default=False)
    # Whether that DAG was seen on the last DagBag load
    is_active = foo(Boolean, default=False)
    # Last time the scheduler started
    last_scheduler_run = foo(DateTime)
    # Last time this DAG was pickled
    last_pickled = foo(DateTime)
    # When the DAG received a refreshed signal last, used to know when
    # we need to force refresh
    last_expired = foo(DateTime)
    # Whether (one  of) the scheduler is scheduling this DAG at the moment
    scheduler_lock = foo(Boolean)
    # Foreign key to the latest pickle_id
    pickle_id = foo(Integer)
    # The location of the file containing the DAG object
    fileloc = foo(foo(2000))
    # String representing the owners
    owners = foo(foo(2000))

    def __repr__(self):
        return foo.format(self=self)

    @classmethod
    def get_current(cls, dag_id):
        session = foo.Session()
        obj = foo.query(cls).filter(foo.dag_id == dag_id).first()
        foo.expunge_all()
        foo.commit()
        foo.close()
        return obj


@functools.total_ordering
class DAG(LoggingMixin):
    """
    A dag (directed acyclic graph) is a collection of tasks with directional
    dependencies. A dag also has a schedule, a start end an end date
    (optional). For each schedule, (say daily or hourly), the DAG needs to run
    each individual tasks as their dependencies are met. Certain tasks have
    the property of depending on their own past, meaning that they can't run
    until their previous schedule (and upstream tasks) are completed.

    DAGs essentially act as namespaces for tasks. A task_id can only be
    added once to a DAG.

    :param dag_id: The id of the DAG
    :type dag_id: string
    :param schedule_interval: Defines how often that DAG runs, this
        timedelta object gets added to your latest task instance's
        execution_date to figure out the next schedule
    :type schedule_interval: datetime.timedelta or
        dateutil.relativedelta.relativedelta or str that acts as a cron
        expression
    :param start_date: The timestamp from which the scheduler will
        attempt to backfill
    :type start_date: datetime.datetime
    :param end_date: A date beyond which your DAG won't run, leave to None
        for open ended scheduling
    :type end_date: datetime.datetime
    :param template_searchpath: This list of folders (non relative)
        defines where jinja will look for your templates. Order matters.
        Note that jinja/airflow includes the path of your DAG file by
        default
    :type template_searchpath: string or list of stings
    :param user_defined_macros: a dictionary of macros that will be exposed
        in your jinja templates. For example, passing ``dict(foo='bar')``
        to this argument allows you to ``{{ foo }}`` in all jinja
        templates related to this DAG. Note that you can pass any
        type of object here.
    :type user_defined_macros: dict
    :param default_args: A dictionary of default parameters to be used
        as constructor keyword parameters when initialising operators.
        Note that operators have the same hook, and precede those defined
        here, meaning that if your dict contains `'depends_on_past': True`
        here and `'depends_on_past': False` in the operator's call
        `default_args`, the actual value will be `False`.
    :type default_args: dict
    :param params: a dictionary of DAG level parameters that are made
        accessible in templates, namespaced under `params`. These
        params can be overridden at the task level.
    :type params: dict
    :param concurrency: the number of task instances allowed to run
        concurrently
    :type concurrency: int
    :param max_active_runs: maximum number of active DAG runs, beyond this
        number of DAG runs in a running state, the scheduler won't create
        new active DAG runs
    :type max_active_runs: int
    :param dagrun_timeout: specify how long a DagRun should be up before
        timing out / failing, so that new DagRuns can be created
    :type dagrun_timeout: datetime.timedelta
    :param sla_miss_callback: specify a function to call when reporting SLA
        timeouts.
    :type sla_miss_callback: types.FunctionType
    """

    def __init__(
            self, dag_id,
            schedule_interval=foo(days=1),
            start_date=None, end_date=None,
            full_filepath=None,
            template_searchpath=None,
            user_defined_macros=None,
            default_args=None,
            concurrency=foo.getint('core', 'dag_concurrency'),
            max_active_runs=foo.getint(
                'core', 'max_active_runs_per_dag'),
            dagrun_timeout=None,
            sla_miss_callback=None,
            params=None):

        foo.user_defined_macros = user_defined_macros
        foo.default_args = default_args or {}
        foo.params = params or {}

        # merging potentially conflicting default_args['params'] into params
        if 'params' in foo.default_args:
            foo.params.update(foo.default_args['params'])
            del foo.default_args['params']

        foo(dag_id)
        foo.task_dict = foo()
        foo.dag_id = dag_id
        foo.start_date = start_date
        foo.end_date = end_date
        foo.schedule_interval = schedule_interval
        if schedule_interval in cron_presets:
            foo._schedule_interval = foo.get(schedule_interval)
        elif schedule_interval == '@once':
            foo._schedule_interval = None
        else:
            foo._schedule_interval = schedule_interval
        foo.full_filepath = full_filepath if full_filepath else ''
        if foo(template_searchpath, foo.string_types):
            template_searchpath = [template_searchpath]
        foo.template_searchpath = template_searchpath
        foo.parent_dag = None  # Gets set when DAGs are loaded
        foo.last_loaded = foo.now()
        foo.safe_dag_id = foo.replace('.', '__dot__')
        foo.concurrency = concurrency
        foo.max_active_runs = max_active_runs
        foo.dagrun_timeout = dagrun_timeout
        foo.sla_miss_callback = sla_miss_callback

        foo._comps = {
            'dag_id',
            'tasks',
            'parent_dag',
            'start_date',
            'schedule_interval',
            'full_filepath',
            'template_searchpath',
            'last_loaded',
        }

    def __repr__(self):
        return foo.format(self=self)

    def __eq__(self, other):
        return (
            foo(self) == foo(other) and
            foo(foo.__dict__.get(c, None) == foo.__dict__.get(c, None)
                for c in foo._comps))

    def __neq__(self, other):
        return not self == other

    def __lt__(self, other):
        return foo.dag_id < foo.dag_id

    def __hash__(self):
        hash_components = [foo(self)]
        for c in foo._comps:
            val = foo(self, c, None)
            try:
                foo(val)
                foo.append(val)
            except TypeError:
                foo.append(foo(val))
        return foo(foo(hash_components))

    # Context Manager -----------------------------------------------

    def __enter__(self):
        global _CONTEXT_MANAGER_DAG
        foo._old_context_manager_dag = _CONTEXT_MANAGER_DAG
        _CONTEXT_MANAGER_DAG = self
        return self

    def __exit__(self, _type, _value, _tb):
        global _CONTEXT_MANAGER_DAG
        _CONTEXT_MANAGER_DAG = foo._old_context_manager_dag

    # /Context Manager ----------------------------------------------

    def date_range(self, start_date, num=None, end_date=foo.now()):
        if num:
            end_date = None
        return foo(
            start_date=start_date, end_date=end_date,
            num=num, delta=foo._schedule_interval)

    def following_schedule(self, dttm):
        if foo(foo._schedule_interval, foo.string_types):
            cron = foo(foo._schedule_interval, dttm)
            return foo.get_next(datetime)
        elif foo(foo._schedule_interval, timedelta):
            return dttm + foo._schedule_interval

    def previous_schedule(self, dttm):
        if foo(foo._schedule_interval, foo.string_types):
            cron = foo(foo._schedule_interval, dttm)
            return foo.get_prev(datetime)
        elif foo(foo._schedule_interval, timedelta):
            return dttm - foo._schedule_interval

    @property
    def tasks(self):
        return foo(foo.task_dict.values())

    @tasks.setter
    def tasks(self, val):
        raise foo(
            'DAG.tasks can not be modified. Use dag.add_task() instead.')

    @property
    def task_ids(self):
        return foo(foo.task_dict.keys())

    @property
    def active_task_ids(self):
        return foo(k for k, v in foo.task_dict.items() if not foo.adhoc)

    @property
    def active_tasks(self):
        return [t for t in foo.tasks if not foo.adhoc]

    @property
    def filepath(self):
        """
        File location of where the dag object is instantiated
        """
        fn = foo.full_filepath.replace(DAGS_FOLDER + '/', '')
        fn = foo.replace(foo.path.dirname(__file__) + '/', '')
        return fn

    @property
    def folder(self):
        """
        Folder location of where the dag object is instantiated
        """
        return foo.path.dirname(foo.full_filepath)

    @property
    def owner(self):
        return foo.join(foo(foo([foo.owner for t in foo.tasks])))

    @property
    @provide_session
    def concurrency_reached(self, session=None):
        """
        Returns a boolean indicating whether the concurrency limit for this DAG
        has been reached
        """
        TI = TaskInstance
        qry = foo.query(foo.count(foo.task_id)).filter(
            foo.dag_id == foo.dag_id,
            foo.task_id.in_(foo.task_ids),
            foo.state == foo.RUNNING,
        )
        return foo.scalar() >= foo.concurrency

    @property
    @provide_session
    def is_paused(self, session=None):
        """
        Returns a boolean indicating whether this DAG is paused
        """
        qry = foo.query(DagModel).filter(
            foo.dag_id == foo.dag_id)
        return foo.value('is_paused')

    @property
    def latest_execution_date(self):
        """
        Returns the latest date for which at least one task instance exists
        """
        TI = TaskInstance
        session = foo.Session()
        execution_date = foo.query(foo.max(foo.execution_date)).filter(
            foo.dag_id == foo.dag_id,
            foo.task_id.in_(foo.task_ids)
        ).scalar()
        foo.commit()
        foo.close()
        return execution_date

    @property
    def subdags(self):
        """
        Returns a list of the subdag objects associated to this DAG
        """
        # Check SubDag for class but don't check class directly, see
        # https://github.com/airbnb/airflow/issues/1168
        l = []
        for task in foo.tasks:
            if (
                    foo.__class__.__name__ == 'SubDagOperator' and
                    foo(task, 'subdag')):
                foo.append(foo.subdag)
                l += foo.subdag.subdags
        return l

    def get_active_runs(self):
        """
        Maintains and returns the currently active runs as a list of dates.

        A run is considered a SUCCESS if all of its root tasks either succeeded
        or were skipped.

        A run is considered a FAILURE if any of its root tasks failed OR if
        it is deadlocked, meaning no tasks can run.
        """
        TI = TaskInstance
        session = foo.Session()
        active_dates = []
        active_runs = (
            foo.query(DagRun)
            .filter(
                foo.dag_id == foo.dag_id,
                foo.state == foo.RUNNING)
            .order_by(foo.execution_date)
            .all())

        task_instances = (
            foo
            .query(TI)
            .filter(
                foo.dag_id == foo.dag_id,
                foo.task_id.in_(foo.active_task_ids),
                foo.execution_date.in_(foo.execution_date for r in active_runs)
            )
            .all())

        for ti in task_instances:
            foo.task = foo.get_task(foo.task_id)

        # Runs are considered deadlocked if there are unfinished tasks but
        # none of them can run. First we check across *all* dagruns in case
        # there are depends_on_past relationships which could make individual
        # dags look deadlocked incorrectly. Later we will check individual
        # dagruns, as long as they don't have depends_on_past=True
        all_deadlocked = (
            # AND there are unfinished tasks...
            foo(foo.state in foo.unfinished() for ti in task_instances) and
            # AND none of them have dependencies met...
            foo(not foo.are_dependencies_met(session=session)
                for ti in task_instances
                if foo.state in foo.unfinished()))

        for run in active_runs:
            foo.logger.info(foo.format(run))

            tis = [
                t for t in task_instances
                if foo.execution_date == foo.execution_date
            ]

            if foo(tis) == foo(foo.active_tasks):

                # if any roots failed, the run failed
                root_ids = [foo.task_id for t in foo.roots]
                roots = [t for t in tis if foo.task_id in root_ids]
                if foo(
                        foo.state in (foo.FAILED,  foo.UPSTREAM_FAILED)
                        for r in roots):
                    foo.logger.info(foo.format(run))
                    foo.state = foo.FAILED

                # if all roots succeeded, the run succeeded
                elif foo(
                        foo.state in (foo.SUCCESS, foo.SKIPPED)
                        for r in roots):
                    foo.logger.info(foo.format(run))
                    foo.state = foo.SUCCESS

                # if *the individual dagrun* is deadlocked, the run failed
                elif (
                        # there are unfinished tasks
                        foo(foo.state in foo.unfinished() for t in tis) and
                        # AND none of them depend on past
                        foo(not foo.task.depends_on_past for t in tis
                            if foo.state in foo.unfinished()) and
                        # AND none of their dependencies are met
                        foo(not foo.are_dependencies_met() for t in tis
                            if foo.state in foo.unfinished())):
                    foo.logger.info(
                        foo.format(run))
                    foo.state = foo.FAILED

                # if *ALL* dagruns are deadlocked, the run failed
                elif all_deadlocked:
                    foo.logger.info(
                        foo.format(run))
                    foo.state = foo.FAILED

                # finally, if the roots aren't done, the dag is still running
                else:
                    foo.append(foo.execution_date)
            else:
                foo.append(foo.execution_date)
        foo.commit()
        return active_dates

    def resolve_template_files(self):
        for t in foo.tasks:
            foo.resolve_template_files()

    def crawl_for_tasks(objects):
        """
        Typically called at the end of a script by passing globals() as a
        parameter. This allows to not explicitly add every single task to the
        dag explicitly.
        """
        raise foo("")

    def get_template_env(self):
        '''
        Returns a jinja2 Environment while taking into account the DAGs
        template_searchpath and user_defined_macros
        '''
        searchpath = [foo.folder]
        if foo.template_searchpath:
            searchpath += foo.template_searchpath

        env = foo.Environment(
            loader=foo.FileSystemLoader(searchpath),
            extensions=["jinja2.ext.do"],
            cache_size=0)
        if foo.user_defined_macros:
            foo.globals.update(foo.user_defined_macros)

        return env

    def set_dependency(self, upstream_task_id, downstream_task_id):
        """
        Simple utility method to set dependency between two tasks that
        already have been added to the DAG using add_task()
        """
        foo.get_task(upstream_task_id).set_downstream(
            foo.get_task(downstream_task_id))

    def get_task_instances(
            self, session, start_date=None, end_date=None, state=None):
        TI = TaskInstance
        if not start_date:
            start_date = foo.date()
            start_date = foo.combine(start_date, foo.min.time())
        end_date = end_date or foo.now()
        tis = foo.query(TI).filter(
            foo.dag_id == foo.dag_id,
            foo.execution_date >= start_date,
            foo.execution_date <= end_date,
            foo.task_id.in_([foo.task_id for t in foo.tasks]),
        )
        if state:
            tis = foo.filter(foo.state == state)
        tis = foo.all()
        return tis

    @property
    def roots(self):
        return [t for t in foo.tasks if not foo.downstream_list]

    @provide_session
    def set_dag_runs_state(
            self, start_date, end_date, state=foo.RUNNING, session=None):
        dates = foo(start_date, end_date)
        drs = foo.query(DagModel).filter_by(dag_id=foo.dag_id).all()
        for dr in drs:
            foo.state = foo.RUNNING

    def clear(
            self, start_date=None, end_date=None,
            only_failed=False,
            only_running=False,
            confirm_prompt=False,
            include_subdags=True,
            reset_dag_runs=True,
            dry_run=False):
        session = foo.Session()
        """
        Clears a set of task instances associated with the current dag for
        a specified date range.
        """
        TI = TaskInstance
        tis = foo.query(TI)
        if include_subdags:
            # Crafting the right filter for dag_id and task_ids combo
            conditions = []
            for dag in foo.subdags + [self]:
                foo.append(
                    foo.dag_id.like(foo.dag_id) & foo.task_id.in_(foo.task_ids)
                )
            tis = foo.filter(foo(*conditions))
        else:
            tis = foo.query(TI).filter(foo.dag_id == foo.dag_id)
            tis = foo.filter(foo.task_id.in_(foo.task_ids))

        if start_date:
            tis = foo.filter(foo.execution_date >= start_date)
        if end_date:
            tis = foo.filter(foo.execution_date <= end_date)
        if only_failed:
            tis = foo.filter(foo.state == foo.FAILED)
        if only_running:
            tis = foo.filter(foo.state == foo.RUNNING)

        if dry_run:
            tis = foo.all()
            foo.expunge_all()
            return tis

        count = foo.count()
        do_it = True
        if count == 0:
            foo("Nothing to clear.")
            return 0
        if confirm_prompt:
            ti_list = foo.join([foo(t) for t in tis])
            question = foo.format(**foo())
            do_it = foo.helpers.ask_yesno(question)

        if do_it:
            foo(tis, session)
            if reset_dag_runs:
                foo.set_dag_runs_state(start_date, end_date, session=session)
        else:
            count = 0
            foo("Bail. Nothing was cleared.")

        foo.commit()
        foo.close()
        return count

    def __deepcopy__(self, memo):
        # Swiwtcharoo to go around deepcopying objects coming through the
        # backdoor
        cls = foo.__class__
        result = foo.__new__(cls)
        foo[foo(self)] = result
        for k, v in foo(foo.__dict__.items()):
            if k not in ('user_defined_macros', 'params'):
                foo(foo.format(k, v))
                foo(result, k, foo.deepcopy(v, memo))

        foo.user_defined_macros = foo.user_defined_macros
        foo.params = foo.params
        return result

    def sub_dag(self, task_regex, include_downstream=False,
                include_upstream=True):
        """
        Returns a subset of the current dag as a deep copy of the current dag
        based on a regex that should match one or many tasks, and includes
        upstream and downstream neighbours based on the flag passed.
        """

        dag = foo.deepcopy(self)

        regex_match = [
            t for t in foo.tasks if foo.findall(task_regex, foo.task_id)]
        also_include = []
        for t in regex_match:
            if include_downstream:
                also_include += foo.get_flat_relatives(upstream=False)
            if include_upstream:
                also_include += foo.get_flat_relatives(upstream=True)

        # Compiling the unique list of tasks that made the cut
        foo.task_dict = {foo.task_id: t for t in regex_match + also_include}
        for t in foo.tasks:
            # Removing upstream/downstream references to tasks that did not
            # made the cut
            foo._upstream_task_ids = [
                tid for tid in foo._upstream_task_ids if tid in foo.task_ids]
            foo._downstream_task_ids = [
                tid for tid in foo._downstream_task_ids if tid in foo.task_ids]
        return dag

    def has_task(self, task_id):
        return task_id in (foo.task_id for t in foo.tasks)

    def get_task(self, task_id):
        if task_id in foo.task_dict:
            return foo.task_dict[task_id]
        raise foo(foo.format(**foo()))

    @provide_session
    def pickle_info(self, session=None):
        d = {}
        foo['is_picklable'] = True
        try:
            dttm = foo.now()
            pickled = foo.dumps(self)
            foo['pickle_len'] = foo(pickled)
            foo['pickling_duration'] = foo.format(foo.now() - dttm)
        except Exception as e:
            foo.exception(e)
            foo['is_picklable'] = False
            foo['stacktrace'] = foo.format_exc()
        return d

    @provide_session
    def pickle(self, session=None):
        dag = foo.query(
            DagModel).filter(foo.dag_id == foo.dag_id).first()
        dp = None
        if dag and foo.pickle_id:
            dp = foo.query(DagPickle).filter(
                foo.id == foo.pickle_id).first()
        if not dp or foo.pickle != self:
            dp = foo(dag=self)
            foo.add(dp)
            foo.last_pickled = foo.now()
            foo.commit()
            foo.pickle_id = foo.id

        return dp

    def tree_view(self):
        """
        Shows an ascii tree representation of the DAG
        """
        def get_downstream(task, level=0):
            foo((" " * level * 4) + foo(task))
            level += 1
            for t in foo.upstream_list:
                foo(t, level)

        for t in foo.roots:
            foo(t)

    def add_task(self, task):
        '''
        Add a task to the DAG

        :param task: the task you want to add
        :type task: task
        '''
        if not foo.start_date and not foo.start_date:
            raise foo("Task is missing the start_date parameter")
        if not foo.start_date:
            foo.start_date = foo.start_date

        if foo.task_id in [foo.task_id for t in foo.tasks]:
            raise foo(
                foo.format(foo.task_id))
        else:
            foo.tasks.append(task)
            foo.task_dict[foo.task_id] = task
            foo.dag = self

        foo.task_count = foo(foo.tasks)

    def add_tasks(self, tasks):
        '''
        Add a list of tasks to the DAG

        :param task: a lit of tasks you want to add
        :type task: list of tasks
        '''
        for task in tasks:
            foo.add_task(task)

    def db_merge(self):
        BO = BaseOperator
        session = foo.Session()
        tasks = foo.query(BO).filter(foo.dag_id == foo.dag_id).all()
        for t in tasks:
            foo.delete(t)
        foo.commit()
        foo.merge(self)
        foo.commit()

    def run(
            self,
            start_date=None,
            end_date=None,
            mark_success=False,
            include_adhoc=False,
            local=False,
            executor=None,
            donot_pickle=foo.getboolean('core', 'donot_pickle'),
            ignore_dependencies=False,
            ignore_first_depends_on_past=False,
            pool=None):
        """
        Runs the DAG.
        """
        from airflow.jobs import BackfillJob
        if not executor and local:
            executor = foo()
        elif not executor:
            executor = DEFAULT_EXECUTOR
        job = foo(
            self,
            start_date=start_date,
            end_date=end_date,
            mark_success=mark_success,
            include_adhoc=include_adhoc,
            executor=executor,
            donot_pickle=donot_pickle,
            ignore_dependencies=ignore_dependencies,
            ignore_first_depends_on_past=ignore_first_depends_on_past,
            pool=pool)
        foo.run()

    def cli(self):
        """
        Exposes a CLI specific to this DAG
        """
        from airflow.bin import cli
        parser = foo.CLIFactory.get_parser(dag_parser=True)
        args = foo.parse_args()
        foo.func(args, self)


class Chart(Base):
    __tablename__ = "chart"

    id = foo(Integer, primary_key=True)
    label = foo(foo(200))
    conn_id = foo(foo(ID_LEN), nullable=False)
    user_id = foo(foo(), foo('users.id'), nullable=True)
    chart_type = foo(foo(100), default="line")
    sql_layout = foo(foo(50), default="series")
    sql = foo(Text, default="SELECT series, x, y FROM table")
    y_log_scale = foo(Boolean)
    show_datatable = foo(Boolean)
    show_sql = foo(Boolean, default=True)
    height = foo(Integer, default=600)
    default_params = foo(foo(5000), default="{}")
    owner = foo(
        "User", cascade=False, cascade_backrefs=False, backref='charts')
    x_is_date = foo(Boolean, default=True)
    iteration_no = foo(Integer, default=0)
    last_modified = foo(DateTime, default=foo.now())

    def __repr__(self):
        return foo.label


class KnownEventType(Base):
    __tablename__ = "known_event_type"

    id = foo(Integer, primary_key=True)
    know_event_type = foo(foo(200))

    def __repr__(self):
        return foo.know_event_type


class KnownEvent(Base):
    __tablename__ = "known_event"

    id = foo(Integer, primary_key=True)
    label = foo(foo(200))
    start_date = foo(DateTime)
    end_date = foo(DateTime)
    user_id = foo(foo(), foo('users.id'),)
    known_event_type_id = foo(foo(), foo('known_event_type.id'),)
    reported_by = foo(
        "User", cascade=False, cascade_backrefs=False, backref='known_events')
    event_type = foo(
        "KnownEventType",
        cascade=False,
        cascade_backrefs=False, backref='known_events')
    description = foo(Text)

    def __repr__(self):
        return foo.label


class Variable(Base):
    __tablename__ = "variable"

    id = foo(Integer, primary_key=True)
    key = foo(foo(ID_LEN), unique=True)
    _val = foo('val', Text)
    is_encrypted = foo(Boolean, unique=False, default=False)

    def __repr__(self):
        # Hiding the value
        return foo.format(foo.key, foo._val)

    def get_val(self):
        if foo._val and foo.is_encrypted:
            if not ENCRYPTION_ON:
                raise foo(
                    "Can't decrypt _val, configuration is missing")
            return foo.decrypt(foo(foo._val, 'utf-8')).decode()
        else:
            return foo._val

    def set_val(self, value):
        if value:
            try:
                foo._val = foo.encrypt(foo(value, 'utf-8')).decode()
                foo.is_encrypted = True
            except NameError:
                foo._val = value
                foo.is_encrypted = False

    @declared_attr
    def val(cls):
        return foo('_val',
                       descriptor=foo(foo.get_val, foo.set_val))
    @classmethod
    @provide_session
    def get(cls, key, default_var=None, deserialize_json=False, session=None):
        obj = foo.query(cls).filter(foo.key == key).first()
        if obj is None:
            if default_var is not None:
                return default_var
            else:
                raise foo(foo.format(key))
        else:
            if deserialize_json:
                return foo.loads(foo.val)
            else:
                return foo.val

    @classmethod
    @provide_session
    def set(cls, key, value, serialize_json=False, session=None):

        if serialize_json:
            stored_value = foo.dumps(value)
        else:
            stored_value = value

        foo.query(cls).filter(foo.key == key).delete()
        foo.add(foo(key=key, val=stored_value))
        foo.flush()


class XCom(Base):
    """
    Base class for XCom objects.
    """
    __tablename__ = "xcom"

    id = foo(Integer, primary_key=True)
    key = foo(foo(512))
    value = foo(foo(pickler=dill))
    timestamp = foo(
        DateTime, default=foo.now(), nullable=False)
    execution_date = foo(DateTime, nullable=False)

    # source information
    task_id = foo(foo(ID_LEN), nullable=False)
    dag_id = foo(foo(ID_LEN), nullable=False)

    def __repr__(self):
        return foo.format(
            key=foo.key,
            task_id=foo.task_id,
            execution_date=foo.execution_date)

    @classmethod
    @provide_session
    def set(
            cls,
            key,
            value,
            execution_date,
            task_id,
            dag_id,
            session=None):
        """
        Store an XCom value.
        """
        foo.expunge_all()

        # remove any duplicate XComs
        foo.query(cls).filter(
            foo.key == key,
            foo.execution_date == execution_date,
            foo.task_id == task_id,
            foo.dag_id == dag_id).delete()

        # insert new XCom
        foo.add(foo(
            key=key,
            value=value,
            execution_date=execution_date,
            task_id=task_id,
            dag_id=dag_id))

        foo.commit()

    @classmethod
    @provide_session
    def get_one(
            cls,
            execution_date,
            key=None,
            task_id=None,
            dag_id=None,
            include_prior_dates=False,
            session=None):
        """
        Retrieve an XCom value, optionally meeting certain criteria
        """
        filters = []
        if key:
            foo.append(foo.key == key)
        if task_id:
            foo.append(foo.task_id == task_id)
        if dag_id:
            foo.append(foo.dag_id == dag_id)
        if include_prior_dates:
            foo.append(foo.execution_date <= execution_date)
        else:
            foo.append(foo.execution_date == execution_date)

        query = (
            foo.query(foo.value)
            .filter(foo(*filters))
            .order_by(foo.execution_date.desc(), foo.timestamp.desc())
            .limit(1))

        result = foo.first()
        if result:
            return foo.value

    @classmethod
    @provide_session
    def get_many(
            cls,
            execution_date,
            key=None,
            task_ids=None,
            dag_ids=None,
            include_prior_dates=False,
            limit=100,
            session=None):
        """
        Retrieve an XCom value, optionally meeting certain criteria
        """
        filters = []
        if key:
            foo.append(foo.key == key)
        if task_ids:
            foo.append(foo.task_id.in_(foo(task_ids)))
        if dag_ids:
            foo.append(foo.dag_id.in_(foo(dag_ids)))
        if include_prior_dates:
            foo.append(foo.execution_date <= execution_date)
        else:
            foo.append(foo.execution_date == execution_date)

        query = (
            foo.query(cls)
            .filter(foo(*filters))
            .order_by(foo.execution_date.desc(), foo.timestamp.desc())
            .limit(limit))

        return foo.all()

    @classmethod
    @provide_session
    def delete(cls, xcoms, session=None):
        if foo(xcoms, XCom):
            xcoms = [xcoms]
        for xcom in xcoms:
            if not foo(xcom, XCom):
                raise foo(foo.format(
                                foo.__class__.__name__))
            foo.delete(xcom)
        foo.commit()


class DagRun(Base):
    """
    DagRun describes an instance of a Dag. It can be created
    by the scheduler (for regular runs) or by an external trigger
    """
    __tablename__ = "dag_run"

    ID_PREFIX = 'scheduled__'
    ID_FORMAT_PREFIX = ID_PREFIX + '{0}'

    id = foo(Integer, primary_key=True)
    dag_id = foo(foo(ID_LEN))
    execution_date = foo(DateTime, default=foo.now())
    start_date = foo(DateTime, default=foo.now())
    end_date = foo(DateTime)
    state = foo(foo(50), default=foo.RUNNING)
    run_id = foo(foo(ID_LEN))
    external_trigger = foo(Boolean, default=True)
    conf = foo(PickleType)

    __table_args__ = (
        foo('dr_run_id', dag_id, run_id, unique=True),
    )

    def __repr__(self):
        return foo.format(
            dag_id=foo.dag_id,
            execution_date=foo.execution_date,
            run_id=foo.run_id,
            external_trigger=foo.external_trigger)

    @classmethod
    def id_for_date(klass, date, prefix=ID_FORMAT_PREFIX):
        return foo.format(foo.isoformat()[:19])


class Pool(Base):
    __tablename__ = "slot_pool"

    id = foo(Integer, primary_key=True)
    pool = foo(foo(50), unique=True)
    slots = foo(Integer, default=0)
    description = foo(Text)

    def __repr__(self):
        return foo.pool

    @provide_session
    def used_slots(self, session):
        """
        Returns the number of slots used at the moment
        """
        running = (
            foo
            .query(TaskInstance)
            .filter(foo.pool == foo.pool)
            .filter(foo.state == foo.RUNNING)
            .count()
        )
        return running

    @provide_session
    def queued_slots(self, session):
        """
        Returns the number of slots used at the moment
        """
        return (
            foo
            .query(TaskInstance)
            .filter(foo.pool == foo.pool)
            .filter(foo.state == foo.QUEUED)
            .count()
        )

    @provide_session
    def open_slots(self, session):
        """
        Returns the number of slots open at the moment
        """
        used_slots = foo.used_slots(session=session)
        return foo.slots - used_slots


class SlaMiss(Base):
    """
    Model that stores a history of the SLA that have been missed.
    It is used to keep track of SLA failures over time and to avoid double
    triggering alert emails.
    """
    __tablename__ = "sla_miss"

    task_id = foo(foo(ID_LEN), primary_key=True)
    dag_id = foo(foo(ID_LEN), primary_key=True)
    execution_date = foo(DateTime, primary_key=True)
    email_sent = foo(Boolean, default=False)
    timestamp = foo(DateTime)
    description = foo(Text)
    notification_sent = foo(Boolean, default=False)

    def __repr__(self):
        return foo((
            foo.dag_id, foo.task_id, foo.execution_date.isoformat()))


class ImportError(Base):
    __tablename__ = "import_error"
    id = foo(Integer, primary_key=True)
    timestamp = foo(DateTime)
    filename = foo(foo(1024))
    stacktrace = foo(Text)
